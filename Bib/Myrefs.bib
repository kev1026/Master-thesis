author = {Chorin, Alexandre J. and Marsden, Jerrold E.},
booktitle = {Performance Evaluation},
doi = {10.1007/978-1-4612-0883-9},
eprint = {arXiv:1011.1669v3},
file = {:home/kaiwen/Library/cmpmath/cfd/Chorin-Marsden-intro-fluid-mechanics.pdf:pdf},
