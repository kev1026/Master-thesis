pages = {120--130},
title = {{Convergence to Steady State Solutions of the Euler Equations on Unstructured Grids with Limiters}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0021999185710844 http://dx.doi.org/10.1006/jcph.1995.1084},
volume = {118},
year = {1995}
}
