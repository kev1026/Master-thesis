keywords = {Characteristic-based interface conditions,Complex configurations,Conservative metric methods,Geometric conservation law,WCNS},
mendeley-groups = {mater-thesis-related/review},
number = {4},
pages = {1081--1102},
title = {{High-order and high accurate CFD methods and their applications for complex grid problems}},
volume = {11},
year = {2012}
}
