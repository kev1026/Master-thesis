@misc{Venkatakrishnan1995,
abstract = {This paper addresses the practical problem of obtaining convergence$\backslash$nto steady state solutions of the Euler equations when limiters are$\backslash$nused in conjunction with upwind schemes on unstructured grids. The$\backslash$nbase scheme forms a gradient and limits it by imposing monotonicity$\backslash$nconditions in the reconstruction stage. It is shown by analysis in$\backslash$none dimension that such an approach leads to various schemes meeting$\backslash$ntotal-variation-diminishing requirements in one dimension. In multiple$\backslash$ndimensions these schemes produce steady-state solutions that are$\backslash$nmonotone and devoid of oscillations. However, convergence stalls$\backslash$nafter a few orders of reduction in the residual. A new limiter is$\backslash$nintroduced that is particularly suited for unstructured grid applications.$\backslash$nWhen reduced to one dimension, it is shown that this limiter satisfies$\backslash$nthe standard theory. With this limiter, it is shown that converged$\backslash$nsteady-state solutions can be obtained. However, the solutions are$\backslash$nnot monotone. There appears to be a conflict between achieving convergence$\backslash$nand monotone solutions with the higher order schemes that employ$\backslash$nlimiters in the framework presented.},
author = {Venkatakrishnan, V.},
booktitle = {J. Comput. Phys.},
